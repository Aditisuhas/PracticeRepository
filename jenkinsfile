pipeline{
    agent any
    stages{
        stage('Clone'){
        steps{
            git "https://github.com/Aditisuhas/PracticeRepository.git"
        }
    }
    stage('Build'){
        steps{
            sh 'echo "Building the project..."'
        }
    }
    stage('Test'){
        steps{
            sh 'python3 -m unittest discover -s ./ -p "test_*.py"'
        }
    }
    

}
}